<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.ql.dao.IProviderDAO">

    <!--全部供应商-->
    <select id="getAllProvider" resultType="smbms_provider">
            select id,proName from smbms_provider
    </select>

    <select id="searchNameById" resultType="String">
       select proName from smbms_provider where id=#{id}
    </select>
</mapper>