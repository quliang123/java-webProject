<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.erma.dao.ClassInfoMapper">
    <select id="getAllData" resultType="Class_info">
        SELECT * from class_info
    </select>


    <insert id="saveClass">
        insert into Class_info(`cname`)value(#{cname})
    </insert>

</mapper>